{
  "overrides": [
    {
      "files": [
        "**/*.{js,mjs,cjs}"
      ],
      "plugins": [
        "@eslint/js"
      ],
      "extends": [
        "plugin:@eslint/js/recommended"
      ]
    },
    {
      "files": [
        "**/*.js"
      ],
      "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
      ],
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "react/prop-types": "off"
      }
    },
    {
      "files": [
        "**/*.{js,mjs,cjs}"
      ],
      "env": {
        "node": true
      }
    }
  ]
}